  },
  "devDependencies": {
    "@rollup/plugin-alias": "^3.1.1",
    "@rollup/plugin-node-resolve": "^9.0.0",
    "decompress": "^4.2.1",
    "glob": "^7.1.6",
    "make-fetch-happen": "^8.0.10",
    "mocha": "^8.4.0",
    "rollup": "^2.75.7",
    "rollup-plugin-cjs-es": "^1.0.1",
    "rollup-plugin-external-globals": "^0.6.1",
    "rollup-plugin-inline-js": "^0.4.0",
